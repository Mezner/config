#!/bin/sh
xrandr --output DP-0 --off --output DP-1 --off --output DP-2 --off --output DP-3 --off --output HDMI-0 --off --output DP-4 --mode 2560x1440 --pos 0x0 --rotate normal --output DP-5 --off --output DP-6 --off --output DP-7 --off --output DP-8 --off

bash ~/.config/polybar/launch.sh --grayblocks
