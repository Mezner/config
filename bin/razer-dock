#!/bin/sh
xrandr --output DP-0 --off --output DP-1 --off --output DP-2 --off --output DP-3 --off --output HDMI-0 --primary --mode 3840x2160 --pos 2560x242 --rotate normal --output DP-4 --off --output DP-5 --mode 2560x1440 --pos 0x602 --rotate normal --output DP-6 --off --output DP-7 --mode 2560x1440 --pos 6400x0 --rotate left --output DP-8 --off

bash ~/.config/polybar/launch.sh --grayblocks
